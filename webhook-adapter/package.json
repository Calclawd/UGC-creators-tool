    "@types/node": "^20.10.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "webhook",
    "agentmail",
    "svix",
    "openclaw",
    "x-outreach"
  ],
  "author": "james",
  "license": "MIT"
}
